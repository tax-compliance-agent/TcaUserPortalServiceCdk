{
  "version": "1.0.0",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "[ -d node_modules ] || npm install && tsc && npm test",
    "clean": "rm -rf dist build",
    "clean:all": "rm -rf dist build node_modules",
    "build:clean": "npm run clean && npm run build",
    "build:clean-all": "npm run clean:all && npm run build",
    "watch": "tsc -w",
    "test": "jest"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "jest": "^30.1.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
